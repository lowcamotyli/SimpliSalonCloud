{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/booksy",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/process-subscriptions",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/check-trial-expirations",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/send-usage-reports",
      "schedule": "0 8 1 * *"
    }
  ]
}